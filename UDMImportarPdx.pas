  cdsCFOP.Open;
  qFilial2.Open;

  tGrupo.Open;
  tCor.Open;
  tCliente.Open;
  tProduto.Open;
  tProdutoCor.Open;
  tFornecedores.Open;
  tTransportadora.Open;
  tCidade.Open;
  tPais.Open;
  tCondPgto.Open;
  tCondPgtoItem.Open;
  tClasFiscal.Open;
  tVendedor.Open;
  tTamanhos.Open;
end;

function TDMImportarPdx.fnc_Abrir_Fornecedor(CNPJ, Nome: String ; ID : Integer): Boolean;
begin
  Result := False;
  cdsFornecedor.Close;
  if ID > 0 then
    sdsFornecedor.CommandText := 'SELECT * FROM PESSOA WHERE CODIGO = ' + IntToStr(ID)
  else
  if Nome <> '' then
    sdsFornecedor.CommandText := 'SELECT * FROM PESSOA WHERE NOME = ' + QuotedStr(Nome)
  else
    sdsFornecedor.CommandText := 'SELECT * FROM PESSOA WHERE CNPJ_CPF = ' + QuotedStr(CNPJ);
  cdsFornecedor.Open;
  if not(cdsFornecedor.IsEmpty) or (cdsFornecedorCNPJ_CPF.AsString = CNPJ) then
    Result := True;
end;

procedure TDMImportarPdx.prc_Abrir_Produto(ID: Integer ; Referencia: String);
begin
  cdsProduto.Close;
  sdsProduto.CommandText := ctCommand_Produto;
  if trim(Referencia) <> '' then
    sdsProduto.CommandText := sdsProduto.CommandText + ' WHERE REFERENCIA = ' + QuotedStr(Referencia)
  else
  if ID <> 0 then
    sdsProduto.CommandText := sdsProduto.CommandText + ' WHERE ID = ' + IntToStr(ID);
  cdsProduto.Open;
end;

function TDMImportarPdx.fnc_Abrir_Cidade(CodMunicipio: String): Boolean;
begin
  Result := False;
  cdsCidade.Close;
  sdsCidade.CommandText := 'SELECT * FROM CIDADE ' +
                           ' WHERE CODMUNICIPIO = ' + QuotedStr(CodMunicipio);
  cdsCidade.Open;
  if not(cdsCidade.IsEmpty) or (cdsCidadeCODMUNICIPIO.AsString = CodMunicipio) then
    Result := True;
end;

function TDMImportarPdx.fnc_Abrir_Pais(CodPais: String): Boolean;
begin
  Result := False;
  cdsPais.Close;
  sdsPais.CommandText := 'SELECT * FROM PAIS '
                       + ' WHERE CODPAIS = ' + QuotedStr(CodPais);
  cdsPais.Open;

  if not(cdsPais.IsEmpty) or (cdsPaisCODPAIS.AsString = CodPais) then
    Result := True;
end;

function TDMImportarPdx.fnc_Abrir_NCM(NCM: String): Integer;
begin
  Result := 0;
  cdsNCM.Close;
  sdsNCM.ParamByName('NCM').AsString := NCM;
  cdsNCM.Open;
  if not(cdsNCM.IsEmpty) then
    Result := cdsNCMID.AsInteger;
end;

function TDMImportarPdx.fnc_Abrir_Transp(Nome,CNPJ: String ; ID: Integer = 0): Boolean;
begin
  Nome := UpperCase(Nome);
  Result := False;
  cdsTransp.Close;
  sdsTransp.CommandText := 'SELECT * FROM PESSOA ';
  if ID > 0 then
    sdsTransp.CommandText := sdsTransp.CommandText
                           + ' WHERE CODIGO = ' + IntToStr(ID)
  else
  if trim(CNPJ) <> '' then
    sdsTransp.CommandText := sdsTransp.CommandText
                           + ' WHERE CNPJ_CPF = ' + QuotedStr(CNPJ)
  else
    sdsTransp.CommandText := sdsTransp.CommandText
                           + ' WHERE NOME = ' + QuotedStr(Nome);
  cdsTransp.Open;
  if not(cdsTransp.IsEmpty) or (cdsTranspNOME.AsString = Nome) then
    Result := True;
end;

function TDMImportarPdx.fnc_Abrir_CSTIPI(CODIPI: String): Integer;
begin
  Result := 0;
  qCSTIPI.Close;
  qCSTIPI.ParamByName('COD_IPI').AsString := CODIPI;
  qCSTIPI.Open;
  if not(qCSTIPI.IsEmpty) and (qCSTIPIID.AsInteger > 0) then
    Result := qCSTIPIID.AsInteger;
end;

procedure TDMImportarPdx.cdsNCMNewRecord(DataSet: TDataSet);
begin
  cdsNCMGERAR_ST.AsString := 'N';
end;

procedure TDMImportarPdx.cdsProdutoNewRecord(DataSet: TDataSet);
begin
  cdsProdutoBAIXA_ESTOQUE_MAT.AsString := 'N';
  cdsProdutoUSA_COR.AsString           := 'N';
  cdsProdutoUSA_GRADE.AsString         := 'N';
end;

procedure TDMImportarPdx.Gravar_ClasFiscal(NCM : String);
var
  vAux: Integer;
begin
  vAux := dmDatabase.ProximaSequencia('TAB_NCM',0);

  cdsNCM.Insert;
  cdsNCMID.AsInteger  := vAux;
  cdsNCMNCM.AsString  := NCM;
  cdsNCMNOME.AsString := '';
  vIDNCM := cdsNCMID.AsInteger;
  cdsNCM.Post;
  cdsNCM.ApplyUpdates(0);
end;

procedure TDMImportarPdx.Gravar_Unidade(Unidade : String);
begin
  if trim(Unidade) = '' then
    exit;

  cdsUnidade.Close;
  sdsUnidade.ParamByName('UNIDADE').AsString := Unidade;
  cdsUnidade.Open;
  if (cdsUnidade.IsEmpty) or (cdsUnidadeUNIDADE.AsString = '') then
  begin
   cdsUnidade.Insert;
   cdsUnidadeUNIDADE.AsString  := Unidade;
   cdsUnidadeNOME.AsString     := '';
   cdsUnidadeCONVERSOR.AsFloat := 0;
   cdsUnidade.Post;
   cdsUnidade.ApplyUpdates(0);
  end;
end;


function TDMImportarPdx.fnc_Gravar_Grade(ID: Integer): Integer;
begin
  Result := 0;
  if not tGrade.Locate('CODIGO',ID,([LocaseInsensitive])) then
    exit;

  cdsGrade.Close;
  sdsGrade.ParamByName('ID').AsInteger := ID;
  cdsGrade.Open;
  if cdsGradeID.AsInteger > 0 then
  begin
    Result := ID;
    exit;
  end;
  Result := ID;

  cdsGrade.Insert;
  cdsGradeID.AsInteger             := ID;
  cdsGradeNOME.AsString            := tGradeNome.AsString;
  cdsGradePOSSUI_MARCACAO.Clear;
  cdsGradeQTD_PARES_TOTAL.AsInteger := tGradeQtdParTotal.AsInteger;
  cdsGradeTIPO_DIG.AsString         := tGradeTipoDig.AsString;
  cdsGradeLARGURA.AsString          := tGradeLargura.AsString;
  cdsGradeCOD_LETRA.AsString        := tGradeCodLetra.AsString;
  cdsGrade.Post;

  tGradeItem.First;
  while not tGradeItem.Eof do
  begin
    cdsGrade_Itens.Insert;
    cdsGrade_ItensID.AsInteger         := tGradeCodigo.AsInteger;
    cdsGrade_ItensTAMANHO.AsString     := tGradeItemTamanho.AsString;
    cdsGrade_ItensTAMANHO_USA.Clear;
    cdsGrade_ItensTAMANHO_EUR.Clear;
    cdsGrade_ItensQTDPAR.AsInteger             := tGradeItemQtdPar.AsInteger;
    cdsGrade_ItensTAM_DEBRUM.AsString          := tGradeItemTamDebrum.AsString;
    cdsGrade_ItensTAM_BIQUEIRA.AsString        := tGradeItemTamBiqueira.AsString;
    cdsGrade_ItensTAM_PALM_INTERNA.AsString    := tGradeItemTamPalmInterna.AsString;
    cdsGrade_ItensTAM_FORRO.AsString           := tGradeItemTamForro.AsString;
    cdsGrade_ItensTAM_AVESSO.AsString          := tGradeItemTamAvesso.AsString;
    cdsGrade_ItensTAM_CAPA_PLATAFORMA.AsString := tGradeItemTamCapaPlataforma.AsString; 
    cdsGrade_ItensTAM_CAPA_SALTO.AsString      := tGradeItemTamCapaSalto.AsString; 
    cdsGrade_ItensTAM_PRE.AsString             := tGradeItemTamPre.AsString; 
    cdsGrade_ItensTAM_TALONEIRA.AsString       := tGradeItemTamTaloneira.AsString; 
    cdsGrade_ItensTAM_ESPUMA.AsString          := tGradeItemTamEspuma.AsString;
    cdsGrade_Itens.Post;
    tGradeItem.Next;
  end;

  cdsGrade_Itens.ApplyUpdates(0);
  cdsGrade.ApplyUpdates(0);
end;

procedure TDMImportarPdx.Gravar_Material;
var
  vAux: Integer;
  vTexto : String;
begin
  vAux := tMaterialCodigo.AsInteger;

  if cdsProduto.Locate('ID',tMaterialCodigo.AsInteger,([LocaseInsensitive])) then
    exit;
  //if cdsProduto.Locate('NOME',tMaterialNome.AsString,([LocaseInsensitive])) then
  //  exit;

  try
    cdsProduto.Insert;
    cdsProdutoID.AsInteger        := vAux;
    cdsProdutoCOD_ANT.AsString    := tMaterialCodigo.AsString;
    cdsProdutoREFERENCIA.AsString := 'M' + FormatFloat('000000',tMaterialCodigo.AsInteger);
    cdsProdutoNOME.AsString       := tMaterialNome.AsString;
    cdsProdutoPESOLIQUIDO.Clear;
    cdsProdutoPESOBRUTO.Clear;
    if tMaterialInativo.AsBoolean then
      cdsProdutoINATIVO.AsString := 'S'
    else
      cdsProdutoINATIVO.AsString := 'N';
    cdsProdutoID_CSTIPI.Clear;
    cdsProdutoPERC_IPI.AsFloat    := StrToFloat(FormatFloat('0.00',tMaterialIPI.AsFloat));
    cdsProdutoPRECO_CUSTO.AsFloat := StrToFloat(FormatFloat('0.00',tMaterialPrCusto.AsFloat));
    cdsProdutoPRECO_VENDA.AsFloat := StrToFloat(FormatFloat('0.00',0));
    cdsProdutoTIPO_REG.AsString   := 'M';
    cdsProdutoPOSSE_MATERIAL.AsString := tMaterialPosseMat.AsString;
    //if tMaterialEstoque.AsBoolean then
      cdsProdutoESTOQUE.AsString := 'S';
    //else
    //  cdsProdutoESTOQUE.AsString := 'N';
    cdsProdutoMATERIAL_OUTROS.AsString := 'M'; 
    cdsProdutoUSUARIO.AsString := 'Paradox';
    cdsProdutoDTCAD.AsDateTime := Date;
    cdsProdutoHRCAD.AsDateTime := Now;
    cdsProdutoCA.Clear;
    cdsProdutoCOMPLEMENTO.Clear;
    vIDNCM := 0;
    vTexto := Monta_Texto(tMaterialCodClasFiscal.AsString,0);
    if (trim(vTexto) <> '') then
    begin
      vIDNCM := fnc_Abrir_NCM(vTexto);
      if vIDNCM <= 0 then
        Gravar_ClasFiscal(vTexto);
    end;
    if vIDNCM > 0 then
      cdsProdutoID_NCM.AsInteger := vIDNCM;
    cdsProdutoORIGEM_PROD.AsString := tMaterialOrigemProd.AsString;
    cdsProdutoCOMPLEMENTO.AsString := tMaterialComplemento.AsString;
    cdsProdutoPERC_REDUCAOICMS.Clear;
    cdsProdutoTIPO_VENDA.Clear;   
    cdsProdutoPERC_MARGEMLUCRO.Clear;
    if trim(tMaterialUnidade.AsString) <> '' then
    begin
      Gravar_Unidade(tMaterialUnidade.AsString);
      cdsProdutoUNIDADE.AsString := tMaterialUnidade.AsString;
    end
    else
      cdsProdutoUNIDADE.AsString := 'UN';
    cdsProdutoDT_ALTPRECO.AsDateTime := tMaterialDtAlteracao.AsDateTime;
    if tMaterialLancaGrade.AsBoolean then
      cdsProdutoUSA_GRADE.AsString := 'S'
    else
      cdsProdutoUSA_GRADE.AsString := 'N';
    cdsProdutoCOD_ANT.AsString := tMaterialCodigo.AsString;
    if (tMaterialCodGrade.AsInteger > 0) and (tMaterialLancaGrade.AsBoolean) then
    begin
      cdsProdutoID_GRADE.AsInteger := fnc_Gravar_Grade(tMaterialCodGrade.AsInteger);
      if cdsProdutoID_GRADE.AsInteger <= 0 then
        cdsProdutoID_GRADE.Clear;
    end;
    if tMaterialCor.RecordCount > 0 then
      cdsProdutoUSA_COR.AsString := 'S'
    else
      cdsProdutoUSA_COR.AsString := 'N';
    cdsProdutoBAIXA_ESTOQUE_MAT.AsString := 'N';
    cdsProdutoID_CSTICMS.Clear;
    if tMaterialPrecoCor.AsBoolean then
      cdsProdutoUSA_PRECO_COR.AsString := 'S'
    else
      cdsProdutoUSA_PRECO_COR.AsString := 'N';
    cdsProduto.Post;

    if tMaterialCor.RecordCount > 0 then
    begin
      cdsProduto_Comb.Close;
      sdsProduto_Comb.ParamByName('ID').AsInteger := tMaterialCodigo.AsInteger;
      cdsProduto_Comb.Open;
      tMaterialCor.First;
      while not tMaterialCor.Eof do
      begin
        prc_Gravar_Material_Cor;
        tMaterialCor.Next;
      end;
      cdsProduto_Comb.ApplyUpdates(0);
    end;

    if (tMaterialLancaGrade.AsBoolean) and (tMaterialCodGrade.AsInteger > 0) then
    begin
      cdsProduto_Tam.Close;
      sdsProduto_Tam.ParamByName('ID').AsInteger := cdsProdutoID.AsInteger;
      cdsProduto_Tam.Open;

      cdsGrade.Close;
      sdsGrade.ParamByName('ID').AsInteger := cdsProdutoID_GRADE.AsInteger;
      cdsGrade.Open;
      cdsGrade_Itens.Close;
      cdsGrade_Itens.Open;
      cdsGrade_Itens.First;
      while not cdsGrade_Itens.Eof do
      begin
        cdsProduto_Tam.Insert;
        cdsProduto_TamID.AsInteger     := cdsProdutoID.AsInteger;
        cdsProduto_TamTAMANHO.AsString := cdsGrade_ItensTAMANHO.AsString;
        cdsProduto_Tam.Post;
        cdsGrade_Itens.Next;
      end;
      cdsProduto_Tam.ApplyUpdates(0);
    end;

    cdsProduto_Calcado.Close;
    sdsProduto_Calcado.ParamByName('ID').AsInteger := cdsProdutoID.AsInteger;
    cdsProduto_Calcado.Open;
    if cdsProduto_Calcado.IsEmpty then
    begin
      cdsProduto_Calcado.Insert;
      cdsProduto_CalcadoID.AsInteger           := cdsProdutoID.AsInteger;
      cdsProduto_CalcadoTIPO_MAT.AsString      := tMaterialTipoMat.AsString;
      cdsProduto_CalcadoTIPO_SOLADO.AsString   := tMaterialTipoSolado.AsString;
      if tMaterialCalcMaterial.AsBoolean then
        cdsProduto_CalcadoCALCULA_MAT.AsString := 'S'
      else
        cdsProduto_CalcadoCALCULA_MAT.AsString := 'N';
      if tMaterialPre.AsBoolean then
        cdsProduto_CalcadoPRE.AsString := 'S'
      else
        cdsProduto_CalcadoPRE.AsString := 'N';
      if tMaterialImpGradeTalaoAuxiliar.AsBoolean then
        cdsProduto_CalcadoIMP_GRADE_AUX.AsString := 'S'
      else
        cdsProduto_CalcadoIMP_GRADE_AUX.AsString := 'N';
      if tMaterialImpTalaoAuxiliar.AsBoolean then
        cdsProduto_CalcadoIMP_TALAO_AUX.AsString := 'S'
      else
        cdsProduto_CalcadoIMP_TALAO_AUX.AsString := 'N';
      if tMaterialImpCalcMaterial.AsBoolean then
        cdsProduto_CalcadoIMP_CALC_MAT.AsString := 'S'
      else
        cdsProduto_CalcadoIMP_CALC_MAT.AsString := 'N';
      if tMaterialGeraOC.AsBoolean then
        cdsProduto_CalcadoGERAR_OC.AsString := 'S'
      else
        cdsProduto_CalcadoGERAR_OC.AsString := 'N';
      cdsProduto_Calcado.Post;
      cdsProduto_Calcado.ApplyUpdates(0);
    end;

    cdsProduto.ApplyUpdates(0);
  except
    on e: Exception do
    begin
      //ShowMessage('Produto: ' + tMaterialCodigo.AsString);
      raise Exception.Create('Erro ao gravar o Produto: ' + tMaterialCodigo.AsString + #13 + e.Message);
    end;
  end;

end;

procedure TDMImportarPdx.prc_Gravar_Material_Cor;
var
  vItem : Integer;
begin
  if cdsProduto_Comb.Locate('ID_COR_COMBINACAO',tMaterialCorCodCor.AsInteger,([LocaseInsensitive])) then
    exit;
  cdsProduto_Comb.Last;
  vItem := cdsProduto_CombITEM.AsInteger;

  cdsProduto_Comb.Insert;
  cdsProduto_CombID.AsInteger      := cdsProdutoID.AsInteger;
  cdsProduto_CombITEM.AsInteger    := vItem + 1;
  cdsProduto_CombID_COR.Clear;
  cdsProduto_CombNOME.AsString     := tMaterialCorlkNomeCor.AsString;
  cdsProduto_CombINATIVO.AsString  := 'N';
  cdsProduto_CombTIPO_REG.AsString := 'C';
  cdsProduto_CombPRECO_CUSTO.AsFloat := tMaterialCorPrCusto.AsFloat;
  cdsProduto_CombPRECO_VENDA.Clear;
  cdsProduto_CombPERC_MARGEMLUCRO.Clear;
  cdsProduto_CombID_COR_COMBINACAO.AsInteger := tMaterialCorCodCor.AsInteger;
  cdsProduto_CombFOTO.Clear;
  cdsProduto_CombOBSMATERIAL.Clear;
  cdsProduto_CombNOME_FOTO.Clear;
  cdsProduto_Comb.Post;
end;

end.
