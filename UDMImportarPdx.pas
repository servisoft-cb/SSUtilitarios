procedure TDMImportarPdx.DataModuleCreate(Sender: TObject);
begin
  ctCommand_CFOP    := sdsCFOP.CommandText;
  ctCommand_Produto := sdsProduto.CommandText;

  qParametros.Close;
  qParametros.ParamByName('ID').AsInteger := 1;
